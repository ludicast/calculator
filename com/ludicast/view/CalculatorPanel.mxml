<?xml version="1.0" encoding="utf-8"?>
<mx:VBox xmlns:mx="http://www.adobe.com/2006/mxml" width="100%" height="100%">
	<mx:Label id="output" />
	<mx:Script>
		<![CDATA[
			import mx.controls.Button;
			public function getButton(id:Number):Button {
				var button:Button = new Button();
				button.label = id.toString();
				return button;  // NOTE - We are not adding button to GUI Yet!!!
			}
			
			
		]]>
	</mx:Script>
	
</mx:VBox>
